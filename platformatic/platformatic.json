{
  "$schema": "https://platformatic.dev/schemas/v1.40.0/runtime",
  "entrypoint": "backend",
  "allowCycles": false,
  "hotReload": true,
  "autoload": {
    "path": "services",
    "exclude": ["docs"]
  },
  "server": {
    "hostname": "{PLT_SERVER_HOSTNAME}",
    "port": "{PORT}",
    "logger": {
      "level": "{PLT_SERVER_LOGGER_LEVEL}"
    },
    "cors": { "origin": ["http://localhost:5173"] }
  },
  "managementApi": "{PLT_MANAGEMENT_API}"
}
